<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.EditMovieController">
   <children>
      <AnchorPane layoutX="1.0" layoutY="1.0" prefHeight="400.0" prefWidth="600.0" stylesheets="@main.css">
         <children>
            <Label fx:id="labelNewEditSongTitle" layoutX="75.0" layoutY="74.0" text="Title:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="labelNewEditSongFile" layoutX="75.0" layoutY="121.0" prefHeight="26.0" prefWidth="34.0" text="File:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="chooseFileButton" layoutX="470.0" layoutY="121.0" mnemonicParsing="false" onAction="#chooseMP4Button" prefHeight="26.0" prefWidth="92.0" text="Choose" />
            <TextField fx:id="txtFieldTitle" layoutX="135.0" layoutY="74.0" prefHeight="26.0" prefWidth="329.0" />
            <TextField fx:id="txtFieldFile" layoutX="135.0" layoutY="121.0" prefHeight="26.0" prefWidth="329.0" />
            <Button fx:id="btnCancel" layoutX="439.0" layoutY="338.0" mnemonicParsing="false" onAction="#cancelEditMovieButton" text="Cancel" />
            <Button fx:id="btnSave" layoutX="511.0" layoutY="338.0" mnemonicParsing="false" onAction="#saveBtn" prefHeight="26.0" prefWidth="51.0" text="Save" />
            <Label fx:id="NewSongTitle" alignment="CENTER" layoutX="175.0" layoutY="14.0" prefHeight="18.0" prefWidth="249.0" text="Edit Movie" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="labelNewEditSongCategory1" layoutX="59.0" layoutY="162.0" text="Rating: ">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="txtFieldFileRating" layoutX="135.0" layoutY="162.0" prefHeight="26.0" prefWidth="329.0" promptText="0.0" />
            <TextField fx:id="txtFieldId" layoutX="19.0" layoutY="351.0" prefHeight="26.0" prefWidth="0.0" visible="false" />
            <Label fx:id="labelLastViewed" layoutX="24.0" layoutY="207.0" text="Last viewed:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="txtFieldFileLastViewed" layoutX="135.0" layoutY="206.0" prefHeight="26.0" prefWidth="329.0" />
            <TextField fx:id="txtFieldFileDuration" editable="false" layoutX="135.0" layoutY="250.0" prefHeight="26.0" prefWidth="329.0" />
            <Label fx:id="labelDuration" layoutX="43.0" layoutY="250.0" text="Duration:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
